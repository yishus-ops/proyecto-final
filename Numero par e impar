// Online C compiler to run C program online
// programa PareImpar
#include <stdio.h>

int main() {
  int valor = 0;
  int n;
  do{
      printf("Ingrese un numero \n");
      scanf("%d",&n);
      if(n % 2 == 0){
          printf("%d es par \n",n);
      }else{
          printf("%d es impar \n");
      }
      valor = valor + 1;
  }while(valor < 10);
printf("*****termino el ciclo***********");
    return 0;
}
